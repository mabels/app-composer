{
  "name": "app-composer",
  "description": "top level app-composer",
  "version": "2.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "clean": "npx lerna run clean",
    "install": "npx lerna bootstrap && npx lerna run install",
    "build": "npx lerna run build --stream --include-filtered-dependencies",
    "pretest": "npm run build",
    "test": "npx lerna run test --stream --include-filtered-dependencies"
  },
  "devDependencies": {
    "lerna": "^3.6.0"
  }
}
